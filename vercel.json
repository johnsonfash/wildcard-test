{
	"version": 2,
	"buildCommand": "npm run build",
	"installCommand": "npm install",
	"outputDirectory": "/build",
	"builds": [
		{
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
		{
			"src": "server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "server.js"
		}
	]
}